esponse = await axios(`api/searchBar/interpret/${query}`);\n  return response.data;\n};\n\nconst doiSearch = async query => {\n  if (query.includes('doi.org')) {\n    // catches both doi.org and dx.doi.org\n    query = new URL(query).pathname.substr(1);\n  }\n\n  if (query.split('/').length < 2) {\n    return [];\n  }\n\n  let doiResp = null;\n  try {\n    doiResp = await axios.get(`/api/doi/${query}`);\n    return [doiResp.data];\n  } catch (err) {\n    console.log(err);\n    return [];\n  }\n};\n\nexport default function PaperSearchBarContainer({ setBlankReview, handleClickResult, handleClickResultButton }) {\n  const [query, setQuery] = useState('');\n  const [loading, setLoading] = useState(false);\n  const [searchResults, setSearchResults] = useState([]);\n\n  const academicSearch = async searchQuery => {\n    // bail out if no query\n    if (searchQuery.length === 0) {\n      setSearchResults([]);\n      setLoading(false);\n      return;\n    }\n\n    setLoading(true);\n\n    // if searchQuery looks like a DOI, call that API instead of interpretation\n    const apiCall = isDOI(searchQuery) ? doiSearch : interpret;\n    const searchResults = await apiCall(searchQuery);\n    setSearchResults(searchResults);\n    setLoading(false);\n  };\n\n  // need to use useCallback here so that debounce always returns the same object;\n  // otherwise we get infinite re-renders with useEffect\n  const academicSearchThrottled = useCallback(_.debounce(academicSearch, 800), []);\n\n  const handleSearch = searchTerm => {\n    academicSearchThrottled.cancel();\n    setQuery(searchTerm);\n\n    // bail out if search is empty\n    if (searchTerm === '') {\n      setLoading(false);\n      setSearchResults([]);\n      return;\n    }\n\n    // update searchbar value and only then run the search\n    setLoading(true);\n  };\n\n  // search when query changes\n  useEffect(() => {\n    if (query !== '') {\n      academicSearchThrottled(query);\n    }\n  }, [query, academicSearchThrottled]);\n\n  return (\n    <PaperSearchBarView\n      handleSearch={handleSearch}\n      handleClickResult={handleClickResult}\n      handleClickResultButton={handleClickResultButton}\n      searchResults={searchResults}\n      setBlankReview={setBlankReview}\n      loading={loading}\n      query={query}\n    />\n  );\n}\n","/Users/aradmargalit/git/paperaweek/client/src/components/SearchableReviewDisplay/SearchableReviewDisplay-view.js",[],"/Users/aradmargalit/git/paperaweek/client/src/components/ReadingList/ReadingList-view.js",[],"/Users/aradmargalit/git/paperaweek/client/src/components/PaperSearchBar/PaperSearchBar-view.js",[],"/Users/aradmargalit/git/paperaweek/client/src/components/StatBox/index.js",[],"/Users/aradmargalit/git/paperaweek/client/src/components/StatBox/StatBox-redux.js",[],"/Users/aradmargalit/git/paperaweek/client/src/components/StatBox/StatBox-container.js",[],"/Users/aradmargalit/git/paperaweek/client/src/components/StatBox/StatBox-view.js",[],"/Users/aradmargalit/git/paperaweek/client/src/components/FrequencyChart/index.js",[],"/Users/aradmargalit/git/paperaweek/client/src/components/FrequencyChart/FrequencyChart-view.js",[],"/Users/aradmargalit/git/paperaweek/client/src/components/ReviewWizard/FormComponents.js",[],"/Users/aradmargalit/git/paperaweek/client/src/components/Drafts/Drafts-redux.js",[],"/Users/aradmargalit/git/paperaweek/client/src/store.ts",[],"/Users/aradmargalit/git/paperaweek/client/src/App.tsx",[],"/Users/aradmargalit/git/paperaweek/client/src/actions/index.ts",[],"/Users/aradmargalit/git/paperaweek/client/src/reducers/reducer_user.ts",[],"/Users/aradmargalit/git/paperaweek/client/src/actions/user/actions.ts",[],"/Users/aradmargalit/git/paperaweek/client/src/actions/user/types.ts",[],"/Users/aradmargalit/git/paperaweek/client/src/actions/user/index.ts",["315"],"/Users/aradmargalit/git/paperaweek/client/src/hooks.ts",[],{"ruleId":"316","replacedBy":"317"},{"ruleId":"318","replacedBy":"319"},{"ruleId":"316","replacedBy":"317"},{"rul