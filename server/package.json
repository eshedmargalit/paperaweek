    "@types/lodash": "^4.14.165",
    "@types/mongoose": "^5.10.1",
    "@types/passport": "^1.0.4",
    "@types/passport-google-oauth20": "^2.0.4",
    "antd": "4.0.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cognito-express": "^2.0.16",
    "concurrently": "^4.1.2",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "mongoose": "^5.6.11",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.2",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "ts-node": "^9.0.0"
